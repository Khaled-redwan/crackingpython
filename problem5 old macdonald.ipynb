    "def old_macdonald(name):\n",
    "    firstletter = name[0]\n",
    "    inbetween = name[1:3]\n",
    "    fourthletter= name[3]\n",
    "    rest= name[4:]\n",
    "    \n",
    "    return firstletter.upper()+inbetween+fourthletter.upper()+rest"
   ]
